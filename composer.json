{
    "name": "witty/laravel-table-view",
    "description": "Laravel 5 Package for easily displaying table views for Eloquent Collections with search and sort functionality built in.",
    "keywords": ["search", "sort", "pagination", "data table", "table view", "laravel", "laravel 5"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Witty\\LaravelTableView\\": "src"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "phpspec/phpspec": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Witty\\LaravelTableView\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Witty\\LaravelTableView\\LaravelTableViewServiceProvider"
            ],
            "aliases": {
                "TableView": "Witty\\LaravelTableView\\Facades\\TableView"
            }
        }
    }
}
